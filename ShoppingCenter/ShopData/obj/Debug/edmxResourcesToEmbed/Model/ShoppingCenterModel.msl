      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Subcategories">
      <EntityTypeMapping TypeName="ShoppingCenterModel.Subcategory">
        <MappingFragment StoreEntitySet="Subcategory">
